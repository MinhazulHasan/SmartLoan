
@{
    ViewData["Title"] = "Inactive";
    Layout = null;
}
<style>
    .center{
        height:100vh;
        display:flex;
        flex-flow: column;
        justify-content:center;
        align-items: center;
    }

    .btn {
        display: inline-block;
        font-weight: 400;
        text-align: center;
        white-space: nowrap;
        vertical-align: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        text-decoration: none;
        border: 1px solid transparent;
        padding: 0.375rem 0.75rem;
        font-size: 1rem;
        line-height: 1.5;
        border-radius: 0.25rem;
        transition: color .15s ease-in-out,background-color .15s ease-in-out,border-color .15s ease-in-out,box-shadow .15s ease-in-out;
    }

    .btn-secondary {
        color: #fff;
        background-color: #6c757d;
        border-color: #6c757d;
    }
</style>

<div class="center">
     <h1>Your current status is inactive. Please email us at admin@admin.com </h1>
    <a class="btn btn-secondary" asp-controller="RegistrationAndLogin" asp-action="Login"> Back to login </a>
</div>

